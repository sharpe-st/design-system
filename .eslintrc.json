{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
			"error",
			"tab"
		],
		"no-var": [
			"error"
		]
    },
    "settings": {
		"react": {
			"version": "18.0"
		}
	}
}
